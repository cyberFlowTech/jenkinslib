@Library('jenkinslib') _  
def tools = new org.devops.tools()
def mimo = new org.devops.mimo()


pipeline {
    agent {
        label 'slave1'
    }
    stages{
        stage("检出代码"){
            steps{
                script {
                    tools.GitCheckOut("$params.tag", "$params.url")
                }
            }
        }
        stage("代码扫描"){
            steps{
                script {
                mimo.Sonarqube("$params.Vsersion","$params.url")
                }
            }
           
        }
    }
    
}
